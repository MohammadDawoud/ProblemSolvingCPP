#include <iostream>
#include <stdio.h>
#include <string.h>
#include <algorithm>
#include <vector>
using namespace std;


class car {
public:
	int x,y;
	string name;

	car(int x, int y, string name) {
		this->name=name;
		this->x=x;
		this->y=y;
	}
};


bool compare(int a, int b) {
	return a<=b;
}




int main() {


	car c1(1,2,"david");
	car c2(3,4,"davoud");

	return 0;
}



