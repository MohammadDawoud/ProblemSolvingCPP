#include <iostream>
#include <stdio.h>
#include <string.h>
#include <algorithm>
#include <vector>
using namespace std;
bool compare(int a, int b) {
	return a<=b;
}
int main() {

	vector<int> v (5,0);

	vector<int> a{10,2,3,4,5,6};
	a.push_back(2);
	a.erase(a.begin()+1);
	for(auto x : a)
		cout << x<< ", ";
	return 0;
}


class car {
public:
	int x,y;
	string name;

	car(int x, int y, string name) {
		this->name=name;
		this->x=x;
		this->y=y;
	}
};
